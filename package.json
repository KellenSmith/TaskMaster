    "prisma-seed-users": "tsx prisma/seed-users.ts",
    "email": "email dev --dir=./src/app/lib/mail-service/mail-templates --port=3001",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
